<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="580.0" prefWidth="330.0" stylesheets="@assets/main.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.miciaha.rendezvous.controllers.CustomerViewController">
    <AnchorPane prefHeight="612.0" prefWidth="497.0" VBox.vgrow="ALWAYS">
        <Group>
            <Rectangle fill="DODGERBLUE" height="50.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="330.0" />
            <Text fill="WHITE" layoutX="104.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Form">
                <font>
                    <Font size="18.0" />
                </font>
            </Text>
        </Group>
        <Group layoutX="26.0" layoutY="156.05078125">
            <ComboBox fx:id="CountryCmb" layoutY="8.94921875" prefHeight="25.0" prefWidth="174.0" />
            <Text layoutX="1.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Country" />
            <Label fx:id="countryErrorLabel" layoutX="1.0" layoutY="33.94921875" styleClass="error-label" text="Error label" />
        </Group>
        <Group layoutX="25.0" layoutY="87.0">
            <TextField fx:id="nameTF" layoutY="8.0" />
            <Text layoutX="2.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
            <Label fx:id="nameErrorLabel" layoutX="1.0" layoutY="33.0" styleClass="error-label" text="Error label" />
        </Group>
        <Group layoutX="26.0" layoutY="231.05078125">
            <ComboBox fx:id="DivisionCmb" layoutY="7.94921875" prefHeight="25.0" prefWidth="174.0" />
            <Text layoutX="1.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Division" />
            <Label fx:id="divisionErrorLabel" layoutX="1.0" layoutY="32.0" styleClass="error-label" text="Label" />
        </Group>
        <Group layoutX="27.0" layoutY="299.0">
            <TextField fx:id="addressTF" layoutY="7.0" prefHeight="25.0" prefWidth="275.0" />
            <Text layoutX="1.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Address" />
            <Label layoutX="1.0" layoutY="29.0" styleClass="error-label" text="Label" />
        </Group>
        <Group layoutX="26.0" layoutY="368.05078125">
            <TextField fx:id="postTF" layoutY="4.94921875" prefHeight="25.0" prefWidth="81.0" />
            <Text layoutX="1.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Postal Code" />
            <Label layoutX="1.0" layoutY="28.0" styleClass="error-label" text="Label" />
        </Group>
        <Group layoutX="26.0" layoutY="431.0">
            <TextField fx:id="phoneTF" layoutY="14.0" />
            <Text layoutX="1.0" layoutY="8.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone Number" />
            <Label layoutX="1.0" layoutY="36.0" styleClass="error-label" text="Label" />
        </Group>
        <Button fx:id="saveCustomerForm" layoutX="91.0" layoutY="513.0" mnemonicParsing="false" prefWidth="63.0" styleClass="btnSave" text="Save" />
        <Button fx:id="cancelForm" layoutX="187.0" layoutY="513.0" mnemonicParsing="false" styleClass="btnCancel" text="Cancel" />
    </AnchorPane>
</VBox>
